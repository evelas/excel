    "copy-webpack-plugin": "^6.3.2",
    "cross-env": "^7.0.2",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^5.0.1",
    "eslint": "^7.13.0",
    "eslint-config-google": "^0.14.0",
    "eslint-loader": "^4.0.2",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.1",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.6.0",
